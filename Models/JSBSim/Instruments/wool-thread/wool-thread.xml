<?xml version="1.0"?>

<PropertyList>

 <path>wool-thread.ac</path>

 <animation>
   <type>select</type>
     <object-name>wool-thread</object-name>
     <object-name>adhesive</object-name>  
     <condition>
       <property>sim/asw20/with-woolthread</property>  
     </condition>
 </animation>

 <animation>
  <type>texrotate</type>
  <object-name>wool-thread</object-name>
  <property>instrumentation/yawstring</property>
  <factor>4</factor>
  <center>
   <x>0.5</x>
   <y>0.5</y>
   <z>0.5</z>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>


</PropertyList>


